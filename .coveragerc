[run]
source = .
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */node_modules/*
    */front/*
    */venv/*
    */env/*
    */.venv/*
    */mcp_python_install_certificates.py
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
